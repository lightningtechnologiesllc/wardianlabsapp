{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "adam-paterson/oauth2-stripe": "^2.0",
        "campaignmonitor/createsend-php": "^7.1",
        "doctrine/dbal": "^3",
        "doctrine/doctrine-bundle": "^2.15",
        "doctrine/doctrine-migrations-bundle": "^3.4",
        "doctrine/orm": "^3.5",
        "knpuniversity/oauth2-client-bundle": "^2.18",
        "lambdish/phunctional": "^2.1",
        "stripe/stripe-php": "^17.5",
        "symfony/amazon-mailer": "7.3.*",
        "symfony/asset": "7.3.*",
        "symfony/console": "7.3.*",
        "symfony/doctrine-messenger": "7.3.*",
        "symfony/dotenv": "7.3.*",
        "symfony/flex": "^2.8.1",
        "symfony/form": "7.3.*",
        "symfony/framework-bundle": "7.3.*",
        "symfony/http-client": "7.3.*",
        "symfony/mailer": "7.3.*",
        "symfony/mailer-send-mailer": "7.3.*",
        "symfony/messenger": "7.3.*",
        "symfony/monolog-bundle": "^3.10",
        "symfony/runtime": "7.3.*",
        "symfony/security-bundle": "7.3.*",
        "symfony/stimulus-bundle": "^2.28",
        "symfony/twig-bundle": "7.3.*",
        "symfony/uid": "7.3.*",
        "symfony/ux-live-component": "^2.28",
        "symfony/ux-turbo": "^2.28",
        "symfony/validator": "7.3.*",
        "symfony/webpack-encore-bundle": "^2.2",
        "symfony/yaml": "7.3.*",
        "wohali/oauth2-discord-new": "^1.2"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "lib/modules/src/",
            "App\\Core\\": "lib/core/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Unit\\App\\": "lib/modules/tests/unit",
            "Tests\\Doubles\\App\\Core\\": "lib/core/tests/doubles",
            "Tests\\Doubles\\App\\": "lib/modules/tests/doubles",
            "Tests\\Acceptance\\App\\": "lib/modules/tests/acceptance",
            "Tests\\Integration\\App\\": "lib/modules/tests/integration",
            "Tests\\Fixtures\\App\\": "lib/modules/tests/fixtures",
            "Tests\\Functional\\App\\": "lib/modules/tests/functional",
            "Tests\\E2E\\App\\": "lib/modules/tests/e2e",
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.3.*"
        }
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^8.4",
        "dbrekelmans/bdi": "^1.4",
        "fakerphp/faker": "^1.24",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^12.3",
        "symfony/browser-kit": "7.3.*",
        "symfony/css-selector": "7.3.*",
        "symfony/maker-bundle": "^1.64",
        "symfony/panther": "^2.2",
        "symfony/stopwatch": "7.3.*",
        "symfony/web-profiler-bundle": "7.3.*"
    }
}
